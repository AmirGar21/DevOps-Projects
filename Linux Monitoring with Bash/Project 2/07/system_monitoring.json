{
  "id": 1,
  "type": "gauge",
  "title": "System monitoring",
  "gridPos": {
    "x": 0,
    "y": 0,
    "h": 8,
    "w": 12
  },
  "fieldConfig": {
    "defaults": {
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "red",
            "value": 80
          }
        ]
      },
      "color": {
        "mode": "thresholds"
      }
    },
    "overrides": [
      {
        "__systemRef": "hideSeriesFrom",
        "matcher": {
          "id": "byNames",
          "options": {
            "mode": "exclude",
            "names": [
              "{__name__=\"node_cpu_seconds_total\", cpu=\"0\", instance=\"localhost:9100\", job=\"prometheus\", mode=\"irq\"}"
            ],
            "prefix": "All except:",
            "readOnly": true
          }
        },
        "properties": []
      }
    ]
  },
  "pluginVersion": "12.0.2",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "eeq0qsi2ch9tsc"
      },
      "disableTextWrap": false,
      "editorMode": "builder",
      "expr": "100 * sum by(instance) (rate(node_cpu_seconds_total{mode!=\"idle\"}[1m])) / sum by(instance) (rate(node_cpu_seconds_total[1m]))\r\n",
      "fullMetaSearch": false,
      "hide": false,
      "includeNullMetadata": false,
      "instant": false,
      "legendFormat": "CPU%",
      "range": true,
      "refId": "A",
      "useBackend": false
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "eeq0qsi2ch9tsc"
      },
      "editorMode": "code",
      "expr": "node_memory_MemAvailable_bytes / 1024 / 1024",
      "hide": false,
      "instant": false,
      "legendFormat": "RAM available (mb)",
      "range": true,
      "refId": "B"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "eeq0qsi2ch9tsc"
      },
      "editorMode": "code",
      "expr": "node_filesystem_avail_bytes{mountpoint=\"/\"} / 1024 / 1024 / 1024",
      "hide": false,
      "instant": false,
      "legendFormat": "Space Available (GB)",
      "range": true,
      "refId": "C"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "eeq0qsi2ch9tsc"
      },
      "editorMode": "code",
      "expr": "sum(rate(node_disk_reads_completed_total[1m]) + rate(node_disk_writes_completed_total[1m]))\r\n",
      "hide": false,
      "instant": false,
      "legendFormat": "I/O",
      "range": true,
      "refId": "D"
    }
  ],
  "datasource": {
    "type": "prometheus",
    "uid": "eeq0qsi2ch9tsc"
  },
  "options": {
    "reduceOptions": {
      "values": false,
      "calcs": [
        "lastNotNull"
      ],
      "fields": ""
    },
    "orientation": "auto",
    "showThresholdLabels": false,
    "showThresholdMarkers": true,
    "sizing": "auto",
    "minVizWidth": 75,
    "minVizHeight": 75
  }
}